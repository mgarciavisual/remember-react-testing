{
	"name": "remember-testing",
	"version": "1.0.0",
	"description": "Remember session of Javascript testing",
	"main": "index.js",
	"scripts": {
		"test": "mocha --compilers js:babel-register --require ./test/browser.js --recursive ./test",
		"test:basic": "mocha --compilers js:babel-register --require ./test/browser.js ./test/1.basic/index.spec.js",
		"test:async": "mocha --compilers js:babel-register --require ./test/browser.js ./test/2.async/index.spec.js",
		"test:fake-timer": "mocha --compilers js:babel-register --require ./test/browser.js ./test/3.fake-timer/index.spec.js",
		"test:spy": "mocha --compilers js:babel-register --require ./test/browser.js ./test/4.spy/index.spec.js",
		"test:stub": "mocha --compilers js:babel-register --require ./test/browser.js ./test/5.stub/index.spec.js",
		"test:fake-server": "mocha --compilers js:babel-register --require ./test/browser.js ./test/6.fake-server/index.spec.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mgarciavisual/remember-testing.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/mgarciavisual/remember-testing/issues"
	},
	"homepage": "https://github.com/mgarciavisual/remember-testing#readme",
	"devDependencies": {
		"babel-core": "^6.18.2",
		"babel-preset-es2015": "^6.18.0",
		"chai": "^3.5.0",
		"jsdom": "^9.8.3",
		"mocha": "^3.1.2",
		"sinon": "^1.17.6"
	},
	"dependencies": {
		"axios": "^0.15.2"
	}
}
